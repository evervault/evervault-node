{
  "name": "@evervault/sdk",
  "version": "6.2.3",
  "description": "Node.js SDK for Evervault",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "prepare": "husky install",
    "lint": "prettier --check \"./**/*.{js,ts}\"",
    "test": "npm run build && mocha 'tests/**/*.test.js' --timeout 30000",
    "test:e2e": "npm run build && mocha 'e2e/**/*.test.js' --timeout 5000 --exit",
    "test:filter": "mocha 'tests/**/*.test.js' --grep",
    "test:coverage": "nyc --reporter=text npm run test",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run generate-types",
    "generate-types": "tsc --declaration --emitDeclarationOnly --outDir dist",
    "build": "tsc",
    "clean": "rm -rf dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/evervault/evervault-node.git"
  },
  "keywords": [
    "node",
    "evervault",
    "enclaves",
    "encryption",
    "cryptography"
  ],
  "author": "Evervault (https://evervault.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/evervault/evervault-node/issues"
  },
  "files": [
    "dist",
    "lib"
  ],
  "homepage": "https://github.com/evervault/evervault-node#readme",
  "dependencies": {
    "agent-base": "^6.0.2",
    "asn1js": "^3.0.5",
    "async-retry": "^1.3.3",
    "axios": "^1.7.0",
    "crc-32": "^1.2.2",
    "uuid": "^8.1.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.7",
    "@types/agent-base": "^4.2.5",
    "@types/node": "^22.10.2",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "husky": "^7.0.2",
    "lint-staged": "^11.1.2",
    "mocha": "^10.0.0",
    "msgpackr": "^1.9.9",
    "nock": "^12.0.3",
    "nyc": "^17.0.0",
    "prettier": "^2.3.2",
    "proxy": "^1.0.2",
    "rewire": "^7.0.0",
    "sinon": "^9.0.2",
    "sinon-chai": "^3.5.0",
    "typescript": "^5.7.2"
  },
  "release": {
    "branches": [
      "master"
    ]
  },
  "lint-staged": {
    "**/*.js": "prettier --write --ignore-unknown \"./**/*.js\""
  }
}
