{
  "name": "@evervault/sdk",
  "version": "0.0.0",
  "description": "Node.js SDK for Evervault",
  "main": "lib/index.js",
  "scripts": {
    "prepare": "husky install",
    "lint": "prettier --check \"./**/*.js\"",
    "test": "mocha 'tests/**/*.test.js'",
    "test:coverage": "nyc --reporter=text npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/evervault/evervault-node.git"
  },
  "keywords": [
    "node",
    "evervault",
    "cages"
  ],
  "author": "Evervault (https://evervault.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/evervault/evervault-node/issues"
  },
  "files": [
    "lib"
  ],
  "homepage": "https://github.com/evervault/evervault-node#readme",
  "dependencies": {
    "agent-base": "^6.0.2",
    "async-retry": "^1.3.3",
    "escodegen": "^1.14.3",
    "esprima": "^4.0.1",
    "haikunator": "^2.1.2",
    "konan": "^2.1.1",
    "lodash": "^4.17.21",
    "phin": "^3.5.0",
    "uasn1": "^0.7.1",
    "uuid": "^8.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "chai": "^4.2.0",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.2.0",
    "husky": "^7.0.2",
    "lint-staged": "^11.1.2",
    "mocha": "^8.4.0",
    "nock": "^12.0.3",
    "nyc": "^15.1.0",
    "prettier": "^2.3.2",
    "proxy": "^1.0.2",
    "rewire": "^5.0.0",
    "sinon": "^9.0.2",
    "sinon-chai": "^3.5.0"
  },
  "release": {
    "branches": [
      "master"
    ]
  },
  "lint-staged": {
    "**/*.js": "prettier --write --ignore-unknown \"./**/*.js\""
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
